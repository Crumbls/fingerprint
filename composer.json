{
    "name": "crumbls/fingerprint",
    "description": "User browser fingerprinting for Laravel.",
    "keywords": [
        "Crumbls",
        "User",
        "Fingerprint",
        "Tracking",
        "Laravel"
    ],
    "homepage": "https://github.com/crumbls/fingerprint",
    "license": "MIT",
    "authors": [
        {
            "name": "Chase C. Miller",
            "email": "chase@crumbls.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20"
    },
    "autoload": {
        "psr-4": {
            "Crumbls\\Fingerprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crumbls\\Fingerprint\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Crumbls\\Fingerprint\\FingerprintServiceProvider"
      ]
    }
  }
}
